{
  "author": "Agoric",
  "devDependencies": {
    "@agoric/eslint-config": "^0.3.24",
    "@endo/eslint-config": "^0.4.10",
    "@jessie.js/eslint-plugin": "^0.1.3",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-jessie": "^0.0.6",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^43.0.8",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@agoric/notifier": "0.6.2",
    "@endo/init": "1.1.9",
    "@endo/promise-kit": "1.1.10",
    "express": "4.19.2",
    "neo4j-driver": "5.28.1",
    "prettier": "3.5.3",
    "semver": "7.6.3",
    "tmp": "0.2.3",
    "zx": "6.2.5"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "license": "Apache-2.0",
  "main": "server.js",
  "name": "@agoric/instagoric-server",
  "packageManager": "yarn@1.22.22",
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "all",
    "singleQuote": true
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "exit 0",
    "dev": "NAMESPACE=${NAMESPACE-instagoric} nodemon server.js",
    "disable-lint:types": "tsc --maxNodeModuleJsDepth 4 -p jsconfig.json",
    "lint": "run-s --continue-on-error lint:*",
    "lint-fix": "yarn lint:eslint --fix",
    "lint:eslint": "eslint --ext .js,.ts .",
    "start": "NAMESPACE=${NAMESPACE-instagoric} node server.js",
    "test": "exit 0",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0"
  },
  "type": "module",
  "version": "1.0.0"
}
