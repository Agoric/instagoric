{
  "name": "@agoric/instagoric-server",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "build": "exit 0",
    "test": "exit 0",
    "start": "NAMESPACE=${NAMESPACE-instagoric} node server.js",
    "test:c8": "c8 $C8_OPTIONS ava --config=ava-nesm.config.js",
    "test:xs": "exit 0",
    "lint-fix": "yarn lint:eslint --fix",
    "lint": "run-s --continue-on-error lint:*",
    "disable-lint:types": "tsc --maxNodeModuleJsDepth 4 -p jsconfig.json",
    "lint:eslint": "eslint --ext .js,.ts ."
  },
  "author": "Agoric",
  "license": "Apache-2.0",
  "devDependencies": {
    "@agoric/eslint-config": "^0.3.24",
    "@endo/eslint-config": "^0.4.10",
    "@jessie.js/eslint-plugin": "^0.1.3",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-jessie": "^0.0.6",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.6.3"
  },
  "dependencies": {
    "@agoric/notifier": "^0.4.0",
    "@endo/init": "^0.5.60",
    "@endo/promise-kit": "^0.2.41",
    "express": "^4.17.1",
    "prettier": "^2.6.2",
    "tmp": "^0.2.1",
    "zx": "^6.1.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "@agoric"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
